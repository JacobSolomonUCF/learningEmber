<div class="list-group" style="background-color:#dfe3ee">
  {{#each model as |user|}}
  <a href="/user/{{user.id}}" class="list-group-item">
    <h4 class="list-group-item-heading">@{{user.username}} - {{user.name}}</h4>
    <a href="mailto:{{user.email}}"><p class="list-group-item-text">{{user.email}}</p></a>
  </a>
  {{/each}}
</div>

{{outlet}}
