{{#each model as |photo|}}
  <div class="row">
    <div class="col-md-6 col-md-3" style="width:100%">
      <div class="thumbnail">
        <img src="{{photo.thumbnailUrl}}" alt="{{photo.url}}" style="height:300px">
        <div class="caption">
          <h3 style="text-align:center">{{photo.title}}</h3>
        </div>
      </div>
    </div>
  </div>
{{/each}}

{{outlet}}
